{
    "compilerOptions": {
        "strict": true,
        "downlevelIteration": true,
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "noEmit": true,
        "resolveJsonModule": true
    }
}
